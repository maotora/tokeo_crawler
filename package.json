    "history": "^4.6.3",
    "jquery": "^1.11.3",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-bootstrap-table": "^3.5.1",
    "react-desktop": "^0.3.1",
    "react-dom": "^15.4.2",
    "react-grid-system": "^2.9.2",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.0.0",
    "redux-actions": "^1.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.3",
